<configuration>
  <system.web>
    <siteMap enabled="true">
      <providers>
        <add name="Web"
             type="System.Web.XmlSiteMapProvider"
             siteMapFile="Web.sitemap"/>
        <add name="Web2"
          type="System.Web.XmlSiteMapProvider"
          siteMapFile="Web2.sitemap"/>
        <add name="Web3"
             type="System.Web.XmlSiteMapProvider"
             siteMapFile="Web3.sitemap"/>
      </providers>
    </siteMap>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>

</configuration>
